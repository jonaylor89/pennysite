# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SECRET_KEY=your-secret-key

# AI Models (at least one required)
# Claude produces better designs - recommended
ANTHROPIC_API_KEY=sk-ant-your-api-key

# OpenAI (fallback if no Anthropic key)
OPENAI_API_KEY=sk-your-api-key

# Stripe Billing
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Stripe Price IDs (create these in Stripe Dashboard)
STRIPE_PRICE_STARTER=price_xxx   # $5 - 50 credits
STRIPE_PRICE_BASIC=price_xxx     # $20 - 220 credits
STRIPE_PRICE_PRO=price_xxx       # $50 - 600 credits
STRIPE_PRICE_MAX=price_xxx       # $100 - 1300 credits

# Credit Pricing (optional - these are defaults)
# BASE_CREDITS_PER_GENERATION=5
# CREDITS_PER_INPUT_TOKEN=0.001
# CREDITS_PER_OUTPUT_TOKEN=0.005
# MAX_RESERVED_CREDITS=150
